{
  "schemaVersion": 1,
  "id": "banMod",
  "version": "${version}",
  "name": "BanMod",
  "description": "Server-wide punishing modification for Minecraft",
  "authors": [
    "Kaleidox"
  ],
  "contact": {
    "homepage": "https://github.com/AMPZNetwork/BanMod",
    "sources": "https://github.com/AMPZNetwork/BanMod"
  },
  "license": "CC0-1.0",
  "icon": "assets/banmod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ampznetwork.banmod.fabric.BanMod$Fabric"
    ],
    "client": []
  },
  "mixins": [
    "banmod-fabric.mixins.json",
    {
      "config": "banmod-fabric.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.9",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*"
